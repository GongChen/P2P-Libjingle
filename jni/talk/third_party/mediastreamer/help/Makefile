install-doc_htmlDATA: $(doc_html_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(doc_htmldir)" || $(MKDIR_P) "$(DESTDIR)$(doc_htmldir)"
	@list='$(doc_html_DATA)'; test -n "$(doc_htmldir)" || list=; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(doc_htmldir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(doc_htmldir)" || exit $$?; \
	done

uninstall-doc_htmlDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(doc_html_DATA)'; test -n "$(doc_htmldir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(doc_htmldir)'; $(am__uninstall_files_from_dir)
